{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "generated from graphql-magic",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec babel-node src",
    "build": "babel src -s -D -d dist ",
    "start": "node dist",
    "prestart": "npm run build -s",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.1.0",
    "body-parser": "^1.15.2",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "graphql": "^0.10.5",
    "graphql-tools": "^1.1.0",
    "helmet": "^3.4.0",
    "lodash": "^4.14.1",
    "morgan": "^1.7.0",
    "resource-router-middleware": "^0.7.0",
    "connect-busboy": "0.0.2",
    "jimp": "^0.2.28",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.22.0",
    "backpack-core": "^0.4.0-rc1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "nodemon": "^1.9.2",
    "chai": "^4.1.2"
  }
}
